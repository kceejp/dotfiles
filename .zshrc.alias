alias vi="vim -p"
alias su="su -p"
alias df="df -h"
alias mkdir="mkdir -p"
alias mv="mv -v"
alias cp="cp -v"
alias rm="rm -iv"
alias ls="ls -FGvh"
alias la="ls -la"
alias ll="ls -l"
alias ack="ack -a"
alias tail="tail -f"
alias tar="tar -v"
alias tree="tree -a"
alias svn="svn --editor-cmd vim"
alias wget="wget --content-disposition"
alias -g L="| less"
alias -g M="| more"
alias -g G="| grep"
alias -g V="| vim -R -"
